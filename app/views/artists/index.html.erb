<h1>Artists</h1>
<p class="artists-index-add-link"><%= link_to "+Add an artist", new_artist_path %></p>
<table>
<% @artists.each do |artist| %>
<tr>
<td><%= image_tag artist.image_url %></td>
<td><h3><%= link_to artist.name, artist_path(artist) %></h3>
    <p><% artist.songs.each do |song| %>
      <%= link_to song.name, artist_song_path(artist,song) %></p>
    <% end %>
<% end %>
</td>
</tr>
</table>
